TOKEN "append" APPEND

# Refer to https://mailgun-docs.redoc.ly/docs/mailgun/user-manual/events/#event-structure

TAGS "mailgun"

SCHEMA >
    `date` DateTime `json:$.timestamp` DEFAULT now(),
    `record` JSON `json:$` DEFAULT '{}'

ENGINE "MergeTree"
ENGINE_PARTITION_KEY "toYYYYMM(date)"
ENGINE_SORTING_KEY "date"