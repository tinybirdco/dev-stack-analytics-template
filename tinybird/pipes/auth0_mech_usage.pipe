TOKEN "read" READ

TAGS "auth0"

NODE ep
SQL >
    %
    SELECT splitByChar('|', event.data.user_id::String)[1] as mech, count() as logins
    FROM auth0
    where event_type == 's'
    AND event_time >= {{DateTime(date_from, '2024-01-01 00:00:00')}}
    AND event_time <= {{DateTime(date_to, '2024-12-31 23:59:59')}}
    group by mech
    order by logins desc


