TOKEN "read" READ

TAGS "auth0"

NODE ep
SQL >
    %
    SELECT toStartOfDay(event_time)::Date as day, count(DISTINCT event.data.user_id) as active
    FROM auth0
    WHERE event_type == 's' 
    AND event_time >= {{DateTime(date_from, '2024-01-01 00:00:00')}}
    AND event_time <= {{DateTime(date_to, '2024-12-31 23:59:59')}}
    GROUP BY day
    ORDER BY day


